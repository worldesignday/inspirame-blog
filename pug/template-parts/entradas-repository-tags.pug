					picture
						source(media="(min-width: 575.98px)" srcset="/assets/images/tag1-img_w_650x487.jpg")
						source(media="(max-width: 576px)" srcset="/assets/images/tag1-img_w_375x280.jpg")
						img(src="/assets/images/tag1-img_w_650x487.jpg" alt="Descripción Imagen" loading="lazy" class="img-fluid rounded")
